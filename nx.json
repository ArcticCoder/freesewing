{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "wbuild", "build:all", "build:sitedeps"],
        "parallel": 5
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build", "prebuild"]
    },
    "wbuild": {
      "dependsOn": ["^wbuild", "prewbuild"]
    },
    "testci": {
      "dependsOn": ["^build"]
    },
    "lint": {
      "dependsOn": []
    },
    "e2e": {
      "dependsOn": ["prebuild"]
    }
  }
}
